@article{Brembilla2019,
abstract = {Climate-Based Daylight Modelling (CBDM) gives designers the possibility to evaluate complex, long-term luminous environment dynamics. This complexity can be challenging to simulate, and even more challenging to communicate effectively through the use of performance metrics. A multiplicity of CBDM techniques and metrics has been developed over the last two decades, but these were rarely assessed against each other. This paper reviews four state-of-the-art techniques based on the Radiance raytracing engine and systematically compares them against a benchmark CBDM method. Four classroom spaces are used to carry out an inter-model comparison between performance metrics commonly used for compliance verification obtained from all analysed techniques. Additional sensitivity analyses assessed how changes in input variables influence such metrics. Results from the inter-model comparison showed that the representation of direct sunlight is markedly different between the various CBDM techniques, and that metrics based on horizontal direct sunlight are very sensitive to the choice of simulation method. This led to differences in predicted Annual Sunlight Exposure up to 39 percentage points. Metrics that consider both direct and inter-reflected light were found to be more robust, with variations from benchmark results within ±15{\%}. The analysis of the input variables showed that sensor grid spacing and time-step interpolation do not significantly affect any of these metrics. Changes in orientation and sky discretisation scheme had different effects depending on the metric and technique considered. The need for authoritative benchmarking systems when introducing new performance metrics for compliance verification or new simulation methods is also discussed.},
author = {Brembilla, E. and Mardaljevic, J.},
doi = {10.1016/j.buildenv.2019.04.051},
file = {:C$\backslash$:/Users/Nuno Saraiva/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Brembilla, Mardaljevic - 2019 - Climate-Based Daylight Modelling for compliance verification Benchmarking multiple state-of-the-art meth.pdf:pdf},
issn = {03601323},
journal = {Building and Environment},
keywords = {Annual daylight metrics,Climate-based daylight modelling,Inter-model comparison,Radiance,Simulation benchmarks},
number = {February},
pages = {151--164},
publisher = {Elsevier},
title = {{Climate-Based Daylight Modelling for compliance verification: Benchmarking multiple state-of-the-art methods}},
volume = {158},
year = {2019}
}

@article{Saxena2010,
abstract = {Existing annual daylight simulation software fall short$\backslash$nwith respect to variable fenestration optics that change$\backslash$ninterior daylight distribution with sun position and/or$\backslash$noperating schedule, thus limiting the ability to compare$\backslash$nthe performance of advanced fenestration systems.$\backslash$nMany of these window or skylight systems can be$\backslash$ndescribed efficiently as a bidirectional scattering$\backslash$ndistribution function (BSDF), which characterizes their$\backslash$nflux output as a function of input for a particular$\backslash$nconfiguration. In this paper, we describe a new method$\backslash$nthat employs measured or simulated BSDFs to permit$\backslash$nfast, matrix-based annual daylighting calculations. The$\backslash$nmatrices themselves are precomputed by Monte Carlo$\backslash$nray-tracing in a modified daylight coefficient approach$\backslash$nwe call Dynamic Radiance. The inner time-step loop$\backslash$nthen consists of multiplying the desired sky luminance$\backslash$nvectors against three matrices in the general case, where$\backslash$na separate BSDF matrix permits dynamic fenestration$\backslash$ncontrol strategies. In this paper, the authors describe$\backslash$ntheir implementation of the Dynamic Radiance method$\backslash$nand demonstrate its application to a set of 61 real spaces$\backslash$nmodeled for a research project to determine new$\backslash$ndaylight metrics. We present results from these$\backslash$nsimulations and discuss advantages and limitations of$\backslash$nthe new approach.},
author = {Saxena, Mudit and Ward, Greg and Perry, T and Heschong, Lisa and Higa, Randall},
file = {:E$\backslash$:/Trabalho/Projetos/On-Going/Doutoramento/Thesis/Document/Literature Review{\_}New/Refer{\^{e}}ncias/Light/SB10-DOC-TS08A-01-Saxena.pdf:pdf},
journal = {In: 4th national conference of IBPSA-USA, 11– 13 August 2010 New York City, New York.},
keywords = {btdf,dynamic,optics,radiance},
mendeley-groups = {10. Light - Buildings and Environment},
pages = {402--409},
title = {{Dynamic Radiance - Predicting Annual Daylighting with Variable Fenestration Optics using BSDFS}},
url = {http://www.ibpsa.us/pub/simbuild2010/papers/SB10-DOC-TS08A-01-Saxena.pdf},
year = {2010}
}

