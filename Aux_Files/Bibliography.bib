@article{Sherman1990,
   abstract = {techniques have become widely used to measure the ventilation rates in buildings. A tracer gas is an idealized substance used to tag volumes of air so as to be able to infer their bulk movement; the properties of a perfect tracer are discussed. The basic principle involved is that of conservation of mass (of both air and tracer gas) as expressed in the continuity equation; by monitoring the injection and concentration of the tracer, one can infer the exchange of air. This report will summarize the techniques in use. Although there is only one continuity equation, there are many different experimental injection strategies and analytical approaches. These different techniques may result in different estimates of ventilation due to uncertainties and biases of the procedures. This report will analyse the spectrum of techniques, including some of the relevant error analyses. NOMENCLATURE C instantaneous tracer gas concentration, mol(ecul)es tracer/mol(ecui)es air C~. inside tracer gas concentration, mol(ecul)es tracer/mol(ecul)es air Cout outside tracer gas concentration, mol(ecul)es tracer/mol(ecul)es air CT target tracer gas concentration, mol(ecul)es tracer/mol(ecul)es air 2 air change rate, h-l qln mass flow of infiltrating air, mol(ecul)es h-qout mass flow of exfiltrating air, mol(ecul) esh-I qr injected mass flow of tracer gas, mol(ecul)es h-1 Q ventilation flow (of air), m 3 h-1 QT instantaneous injection of tracer gas, m 3 h-t time, h T length of measurement period, h V volume, m 3 p density of air in a zone, mol(ecul)es air m-3 6X uncertainty in quantity X (e.g. concentration, injection, etc.) time rate of change of quantity X time average of quantity X over measurement period T XR indicates X is a parameter found from a regression Xt indicates X is found using an integral technique INTRODUCTION},
   author = {M H Sherman},
   issue = {4},
   journal = {Building and Environment},
   pages = {365-374},
   title = {Tracer-Gas Techniques For Measuring Ventilation in a Single Zone},
   volume = {25},
   year = {1990},
}

