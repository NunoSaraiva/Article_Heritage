}


@article{SanchezdelaCampa2011,
abstract = {This work documents for the first time the levels and composition of atmospheric particulate matter in the historic mining district of Rio Tinto (Spain) to estimate the contribution and impact of resuspended particles from hazardous mine waste on air quality. The resuspended mine waste dust contributes notably (32\%) to the total concentrations of toxic trace metals (Bi, As, Cu, Pb, Cd, Zn and Sb) into the atmosphere, with the consequent impact on public health. {\textcopyright} 2011 Elsevier Inc.},
